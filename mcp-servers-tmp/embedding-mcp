#!/bin/bash
# This is a wrapper script for the Python embedding MCP server

VENV_DIR="/home/james/.local/share/gemini-cli/mcp-servers/embedding/venv"
SERVER_SCRIPT="/home/james/.local/share/gemini-cli/mcp-servers/embedding/server.py"

# Activate virtual environment
source "/home/james/.local/share/gemini-cli/mcp-servers/embedding/venv/bin/activate"

# Execute the Python server script
exec python3 "" "gemini-cli-bin"
